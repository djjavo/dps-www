<?php
	class Tracks{
		public static function last(){
			$sue = (Shows::current()->get_showid() == 0);
			$result = DigiplayDB::query("SELECT track_title AS title,track_artist AS artist FROM log WHERE ".($sue?"userid = 1":"userid != 1")." AND datetime > '".(time()-(5*60))."' ORDER BY datetime DESC LIMIT 1;");
			if(pg_num_rows($result)==1)
				return pg_fetch_object($result,null,"Track");
			else
				return false;
		}
	}
?>